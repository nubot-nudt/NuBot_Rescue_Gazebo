<?xml version="1.0"?>
<package>
  <name>nubot_launch</name>
  <version>0.0.0</version>
  <description>
    nubot_launch contains launch files for launching pumbaa_gazebo.
  </description>
  <maintainer email="skywalker1941@163.com">Skywalker1941</maintainer>
  <author email="skywalker1941@163.com">Skywalker1941</author>
  <license>BSD</license>


