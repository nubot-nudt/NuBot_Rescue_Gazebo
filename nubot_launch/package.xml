  <run_depend>gazebo_description</run_depend>
  <run_depend>nubot_pumbaa_plugin</run_depend>
  <run_depend>nubot_teleop</run_depend>
  <run_depend>nubot_msgs</run_depend>

  <run_depend>velodyne_simulator</run_depend>
  <run_depend>hector_gazebo</run_depend>

